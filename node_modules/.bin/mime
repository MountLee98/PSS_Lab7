#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/mime@2.5.2/node_modules/mime/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/mime@2.5.2/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules:/mnt/c/eclipse-2018-09/workspace/node_modules:/mnt/c/eclipse-2018-09/node_modules:/mnt/c/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/mime/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
