#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/regjsparser@0.1.5/node_modules/regjsparser/bin/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/regjsparser@0.1.5/node_modules/regjsparser/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/regjsparser@0.1.5/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules:/mnt/c/eclipse-2018-09/workspace/node_modules:/mnt/c/eclipse-2018-09/node_modules:/mnt/c/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/regjsparser/bin/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/regjsparser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../regjsparser/bin/parser" "$@"
else
  exec node  "$basedir/../regjsparser/bin/parser" "$@"
fi
