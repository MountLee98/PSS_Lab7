#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/bin/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/mkdirp@0.5.5/node_modules/mkdirp/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/mkdirp@0.5.5/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/.pnpm/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules:/mnt/c/eclipse-2018-09/workspace/node_modules:/mnt/c/eclipse-2018-09/node_modules:/mnt/c/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/mkdirp/bin/node_modules:/mnt/c/eclipse-2018-09/workspace/pai/node_modules/mkdirp/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
